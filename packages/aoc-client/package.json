{
	"name": "@bryan-hoang/aoc-client",
	"version": "1.0.0",
	"description": "An Advent of Code client library",
	"license": "MIT",
	"author": "Bryan Hoang <bryan@bryanhoang.dev> (https://bryanhoang.dev/)",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"build:stub": "unbuild --stub",
		"prepack": "unbuild",
		"test": "vitest",
		"test:attw": "attw --pack ."
	},
	"dependencies": {
		"consola": "catalog:",
		"cookie-es": "catalog:",
		"debug": "catalog:",
		"defu": "catalog:",
		"env-paths": "catalog:",
		"ofetch": "catalog:",
		"pathe": "catalog:",
		"temporal-polyfill": "catalog:",
		"turndown": "catalog:",
		"type-level-regexp": "catalog:"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "catalog:",
		"@total-typescript/tsconfig": "catalog:",
		"@types/debug": "catalog:",
		"@types/turndown": "catalog:",
		"unbuild": "catalog:",
		"unplugin-purge-polyfills": "catalog:",
		"vitest": "catalog:"
	},
	"publishConfig": {
		"access": "public"
	}
}
